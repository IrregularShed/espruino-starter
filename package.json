{
  "name": "espruino-starter",
  "version": "1.0.0",
  "description": "starter repo to build and deploy modular and extended Espruino code",
  "main": "build/espruino.js",
  "scripts": {
    "build": "node_modules/.bin/rollup -c",
    "wipe": "espruino --port COM23 -b 115200 --no-ble --board ./esp32board.json ./utils/wipe.js",
    "reset": "espruino --port COM23 -b 115200 --no-ble --board ./esp32board.json ./utils/reset.js",
    "upload": "espruino --port COM23 -b 115200 --no-ble --board ./esp32board.json ./build/espruino.dev.js",
    "dev": "npm run build && npm run wipe && npm run upload --isdev=.dev && npm run term",
    "pro": "PRODUCTION=true npm run build && espruino --port COM23 -b 115200 --no-ble --board ./esp32board.json ./build/espruino.js",
    "term": "espruino --port COM23 -b 115200 --no-ble --board ./esp32board.json"
  },
  "author": "dkebler",
  "contributors": [
    {
      "name": "Steve Anderson",
      "email": "steve@twindx.com",
      "url": "https://twindx.com"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-yaml": "^2.1.1",
    "@types/espruino": "^1.94.7",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "eslint": "^7.8.1",
    "eslint-config-espruino-globals": "^1.0.0",
    "espruino": "^0.1.45",
    "rollup": "^2.79.1",
    "rollup-plugin-inject-process-env": "^1.3.0",
    "rollup-plugin-terser": "^7.0.0"
  },
  "dependencies": {
    "await-to-js": "^2.1.1"
  }
}
